<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-06-12T03:38:19">
   <properties>
      <titles>
         <title>Chorus52 Kehi Kurama Chintit</title>
      </titles>
      <authors>
         <author>Nepali Christain Bhajan</author>
      </authors>
      <songbooks>
         <songbook name="bhajan" entry="1"/>
      </songbooks>
      <verseOrder/>
   </properties>
   <lyrics>
      <verse name="v1">
         <lines>
            केही कुरामा चिन्तित नहोओ,
            <br/>
            सबै कुरामा धन्यवादसाथ प्रार्थना गर 
            <br/>
             परमेश्वरको शान्तिले तिम्रो हृदय
            <br/>
            र तिम्रो विचारलाई येशूमा रक्षा गर्नेछ ।
         </lines>
      </verse>
   </lyrics>
</song>
