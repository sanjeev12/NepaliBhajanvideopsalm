   <properties>
      <titles>
         <title>Chorus2 Jiudo Chan Yeshu</title>
      </titles>
      <authors>
         <author>Nepali Christain Bhajan</author>
      </authors>
      <songbooks>
         <songbook name="bhajan" entry="1"/>
      </songbooks>
      <verseOrder/>
   </properties>
   <lyrics>
      <verse name="v1">
         <lines>
            जिउँदो छन्, येशू जिउँदो छन्,
            <br/>
            मेरो मनमा येशू जिउँदो छन्
            <br/>
            संकटमा मेरो सहारा हुन्,
            <br/>
            येशू सधैँ जिउँदो छन् ।
         </lines>
      </verse>
   </lyrics>
