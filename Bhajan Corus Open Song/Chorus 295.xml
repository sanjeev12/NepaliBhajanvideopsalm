<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-06-12T03:38:19">
   <properties>
      <titles>
         <title>Chorus295 Jaba Ma Tapiko Sihasantarfa</title>
      </titles>
      <authors>
         <author>Nepali Christain Bhajan</author>
      </authors>
      <songbooks>
         <songbook name="bhajan" entry="1"/>
      </songbooks>
      <verseOrder/>
   </properties>
   <lyrics>
      <verse name="v1">
         <lines>
            जब म तपाईंको सिंहासनतर्फ हेर्छु (४)
            <br/>
            म आनन्दले भरपूर बन्छु (२) 
            <br/>
             जब म मेरो मुकुट त्यहाँ देख्छु,
            <br/>
            निश्चय बन्छु म त्यो पाउनेछु (२)
         </lines>
      </verse>
   </lyrics>
</song>
