<?xml version="1.0" ?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-06-12T03:38:19">
   <properties>
      <titles>
         <title>Chorus303 Tapai Yogya</title>
      </titles>
      <authors>
         <author>Nepali Christain Bhajan</author>
      </authors>
      <songbooks>
         <songbook name="bhajan" entry="1"/>
      </songbooks>
      <verseOrder/>
   </properties>
   <lyrics>
      <verse name="v1">
         <lines>
            तपाईं योग्य, तपाईं योग्य, प्रभु हुनुहुन्छ
            <br/>
            महिमा पाउन, महिमा आदर, महिमा आदर, शक्ति 
            <br/>
             सारा सृष्टि तपाईंले सृजनुभयो, तपाईंले सृजनुभो
            <br/>
            तपाईंकै इच्छामा सृजना भए, तपाईं योग्य प्रभु ।
         </lines>
      </verse>
   </lyrics>
</song>
