<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-06-12T03:38:19">
   <properties>
      <titles>
         <title>Chorus307 Badhau Haami Badhau</title>
      </titles>
      <authors>
         <author>Nepali Christain Bhajan</author>
      </authors>
      <songbooks>
         <songbook name="bhajan" entry="1"/>
      </songbooks>
      <verseOrder/>
   </properties>
   <lyrics>
      <verse name="v1">
         <lines>
            बढ़ौँ हामी बढ़ौँ, एकै ढिक्का भई (२)
            <br/>
            विजय हाम्रो येशूमा निश्चय हुनेछ है (२) हो (२) 
            <br/>
             निकाली सबले एउटै आवाज शैतानलाई डाँटौँ है (२)
            <br/>
            (त्यो पर) ६ हामीदेखि टाढ़ा नै है (२) येशूको नाउँमा (२)
         </lines>
      </verse>
   </lyrics>
</song>
