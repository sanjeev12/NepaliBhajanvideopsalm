   <properties>
      <titles>
         <title>Bal Chorus14 Jasko Gharma Yeshu Cha</title>
      </titles>
      <authors>
         <author>Nepali Christain Bhajan</author>
      </authors>
      <songbooks>
         <songbook name="bhajan" entry="1"/>
