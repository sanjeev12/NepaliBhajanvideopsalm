<?xml version="1.0" ?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-06-12T03:38:19">
   <properties>
      <titles>
         <title>Chorus72 Aradhana Garchu Mahan</title>
      </titles>
      <authors>
         <author>Nepali Christain Bhajan</author>
      </authors>
      <songbooks>
         <songbook name="bhajan" entry="1"/>
      </songbooks>
      <verseOrder/>
   </properties>
   <lyrics>
      <verse name="v1">
         <lines>
            आराधना गर्छु, महान् प्रभु
            <br/>
            तपाईंझैँ छैन अरू 
            <br/>
             आराधना गर्छु, शान्तिका कुमार
            <br/>
            यसै गर्न चाहन्छु  
            <br/>
             म स्तुति गर्छु, तपाईं मेरो धार्मिकता,
            <br/>
            आराधना गर्छु महान् प्रभु, तपाईंझैँ छैन अरू ।
         </lines>
      </verse>
   </lyrics>
</song>
