<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-06-12T03:38:19">
   <properties>
      <titles>
         <title>Chorus41 Dhanyabadi Bhae Uhako</title>
      </titles>
      <authors>
         <author>Nepali Christain Bhajan</author>
      </authors>
      <songbooks>
         <songbook name="bhajan" entry="1"/>
      </songbooks>
      <verseOrder/>
   </properties>
   <lyrics>
      <verse name="v1">
         <lines>
            धन्यवादी भई उहाँको ढोकाभित्र पस्नेछु,
            <br/>
            आँगनभित्र प्रशंसासाथ, 
            <br/>
             भत्रेछु यो त्यही दिन जो प्रभुले बनाउनुभो,
            <br/>
            खुशी छु, उहाँले (खुशी पार्नुभो) २,
            <br/>
            (खुशी छु उहाँले खुशी पार्नुभो) २
         </lines>
      </verse>
   </lyrics>
</song>
