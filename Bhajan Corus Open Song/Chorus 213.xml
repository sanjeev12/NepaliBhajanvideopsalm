<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-06-12T03:38:19">
   <properties>
      <titles>
         <title>Chorus213 Paramprabhu Raj Garnuhuncha</title>
      </titles>
      <authors>
         <author>Nepali Christain Bhajan</author>
      </authors>
      <songbooks>
         <songbook name="bhajan" entry="1"/>
      </songbooks>
      <verseOrder/>
   </properties>
   <lyrics>
      <verse name="v1">
         <lines>
            परमप्रभु राज गर्नुहुन्छ सदा,
            <br/>
            सियोनमा सदा विराजमान् (२) 
            <br/>
             उनको राज्यको घोषणा गरौँ,
            <br/>
            उनको राज्यमा सबै रमाउनेछन् (२) 
            <br/>
             इन्साफ र आनन्द र धार्मिकता,
            <br/>
            हुनेछ उनको राज्यमा सदा (२)
         </lines>
      </verse>
   </lyrics>
</song>
