      <titles>
         <title>Bal Chorus30 Suna Naani Suna Yeshuko Sandes</title>
      </titles>
      <authors>
         <author>Nepali Christain Bhajan</author>
      </authors>
      <songbooks>
         <songbook name="bhajan" entry="1"/>
