<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-06-12T03:38:19">
   <properties>
      <titles>
         <title>Chorus101 Tapai Mero Bal Prabhu</title>
      </titles>
      <authors>
         <author>Nepali Christain Bhajan</author>
      </authors>
      <songbooks>
         <songbook name="bhajan" entry="1"/>
      </songbooks>
      <verseOrder/>
   </properties>
   <lyrics>
      <verse name="v1">
         <lines>
            तपाईं मेरो बल प्रभु, तपार्इं मेरो सहायता,
            <br/>
            तपाईं जसलाई म पुकार्छु, 
            <br/>
             तपाईं मेरो ढाल, प्रभु, मेरो जीवन अर्पन्छु,
            <br/>
            कारण तपाईं सर्वेसर्वा मेरो ।
         </lines>
      </verse>
   </lyrics>
</song>
