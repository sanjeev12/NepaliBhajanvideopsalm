<?xml version="1.0" ?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-06-12T03:38:19">
   <properties>
      <titles>
         <title>Chorus27 Aajako Din Aajako Din</title>
      </titles>
      <authors>
         <author>Nepali Christain Bhajan</author>
      </authors>
      <songbooks>
         <songbook name="bhajan" entry="1"/>
      </songbooks>
      <verseOrder/>
   </properties>
   <lyrics>
      <verse name="v1">
         <lines>
            आजको दिन, आजको दिन
            <br/>
            उनले सिर्जेका, उनले सिर्जेका 
            <br/>
             रमाउँदै जाऊँ, खुशी मनाऊँ (र आनन्द गरौँ) २ 
            <br/>
             आजको दिन उनले सिर्जेका,
            <br/>
            रमाउँदै जाऊँ, र खुशी मनाऊँ 
            <br/>
             आजको दिन (२) उनले सिर्जेका ।
         </lines>
      </verse>
   </lyrics>
</song>
