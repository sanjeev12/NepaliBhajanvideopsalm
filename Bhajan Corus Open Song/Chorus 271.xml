<?xml version="1.0" ?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-06-12T03:38:19">
   <properties>
      <titles>
         <title>Chorus271 Stuti Aaradhana Mathi</title>
      </titles>
      <authors>
         <author>Nepali Christain Bhajan</author>
      </authors>
      <songbooks>
         <songbook name="bhajan" entry="1"/>
      </songbooks>
      <verseOrder/>
   </properties>
   <lyrics>
      <verse name="v1">
         <lines>
            स्तुति आराधना माथि जानेछ,
            <br/>
            आशिष् लिएर तल आउनेछ 
            <br/>
             प्रभु हाम्रा कति महान्,
            <br/>
            हेर हामीलाई गर्छन् प्यार,
            <br/>
            हल्लेलू, हल्लेलूयाह (२) 
            <br/>
             प्रार्थना विन्ति माथि जानेछ,
            <br/>
            उत्तर लिएर तल आउनेछ 
            <br/>
             प्रभु हाम्रा कति महान्,
            <br/>
            हेर हामीलाई गर्छन् प्यार,
            <br/>
            हल्लेलू, हल्लेलूयाह (२)
         </lines>
      </verse>
   </lyrics>
</song>
