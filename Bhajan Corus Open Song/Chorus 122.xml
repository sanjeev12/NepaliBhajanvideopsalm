<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-06-12T03:38:19">
   <properties>
      <titles>
         <title>Chorus122 Aawo Dandwaat Garau</title>
      </titles>
      <authors>
         <author>Nepali Christain Bhajan</author>
      </authors>
      <songbooks>
         <songbook name="bhajan" entry="1"/>
      </songbooks>
      <verseOrder/>
   </properties>
   <lyrics>
      <verse name="v1">
         <lines>
            आओ दण्डवत् गरौँ र निहुरौँ
            <br/>
            हाम्रा सृष्टिकर्ता परमप्रभुकै सामु घुँड़ा टेकौँ 
            <br/>
             किनभने उहाँ नै हाम्रा परमेश्वर हुनुहुन्छ
            <br/>
            र हामी उहाँका खर्कका प्रजा, उहाँका हातका भेड़ा हौँ ।
         </lines>
      </verse>
   </lyrics>
</song>
