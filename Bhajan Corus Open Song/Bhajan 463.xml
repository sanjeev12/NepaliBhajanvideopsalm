<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-06-12T03:38:19">
   <properties>
      <titles>
         <title>Bhajan463 Yeshu Bhanchan Balakharulai</title>
      </titles>
      <authors>
         <author>Nepali Christain Bhajan</author>
      </authors>
      <songbooks>
         <songbook name="bhajan" entry="1"/>
      </songbooks>
      <verseOrder>v1 v2 </verseOrder>
   </properties>
   <lyrics>
      <verse name="v1">
         <lines>
            १. येशू भन्छन् बालकहरूलाई म कहाँ आउँन देऊ
            <br/>
            किनभने स्वर्गको राज्य यस्तैनैहरूको हो । 
         </lines>
      </verse>
      <verse name="v2">
         <lines>
             २. यसकारण हामी सबै मिलेर उनको चरणमा जाऔं
            <br/>
            उनको चरणमा गएर हामी मीठो आवाजलाई सुनाऔं ।
         </lines>
      </verse>
   </lyrics>
</song>
