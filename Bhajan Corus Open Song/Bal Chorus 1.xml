   <properties>
      <titles>
         <title>Bal Chorus1 Sanibarko Bihan</title>
      </titles>
      <authors>
         <author>Nepali Christain Bhajan</author>
      </authors>
      <songbooks>
         <songbook name="bhajan" entry="1"/>
      </songbooks>
      <verseOrder/>
   </properties>
   <lyrics>
      <verse name="v1">
         <lines>
            शनिबारको बिहान साथीसङ्गी आए
            <br/>
            खेल्न डुल्न जाऔँ भनी तात्र मलाई लागे,
            <br/>
            जात्रँ जात्रँ खेल्न डुल्न, जान्छु सन्डे स्कूलमा (२)
            <br/>
            येशूले बोलाएका आवाज सुत्र (२)
         </lines>
      </verse>
   </lyrics>
</song>
