   <properties>
      <titles>
         <title>Chorus103 Sara Aadar Sara Mahima</title>
      </titles>
      <authors>
         <author>Nepali Christain Bhajan</author>
      </authors>
      <songbooks>
         <songbook name="bhajan" entry="1"/>
      </songbooks>
      <verseOrder/>
   </properties>
   <lyrics>
      <verse name="v1">
         <lines>
            सारा आदर, सारा महिमा,
            <br/>
            सारा शक्ति तपाईंलाई (२) 
            <br/>
             पवित्र पिता, आराधना गर्छौं,
            <br/>
            अनमोल येशू हाम्रो उद्धारक, 
            <br/>
             (पवित्र आत्मा, बाटो हेर्छौं) २
            <br/>
            (जागृतिको लागि) ३
         </lines>
      </verse>
   </lyrics>
</song>
