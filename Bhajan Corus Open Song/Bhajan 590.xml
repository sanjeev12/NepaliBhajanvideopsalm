<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-06-12T03:38:19">
   <properties>
      <titles>
         <title>Bhajan590 Timro Lagi Susandesh</title>
      </titles>
      <authors>
         <author>Nepali Christain Bhajan</author>
      </authors>
      <songbooks>
         <songbook name="bhajan" entry="1"/>
      </songbooks>
      <verseOrder>c1 v1 c1 v2 c1 </verseOrder>
   </properties>
   <lyrics>
      <verse name="c1">
         <lines>कोः तिम्रो लागि सुसन्देश छ है (३) यो समयमा (२) </lines>
      </verse>
      <verse name="v1">
         <lines>
             १. कठ्याङ्ग्रिँदो यो जाडोमा मुक्तिदाता पैदा हुनुभो
            <br/>
            चम्कियो ज्योति संसारभरि मुक्तिदाताको जन्म दिनमा । 
         </lines>
      </verse>
      <verse name="v2">
         <lines>
             २. स्वर्गको वैभवलाई त्यागेर आउनुभयो येशू संसारमा
            <br/>
            विश्वास येशूमा गर न, पाउनेछौ अन्नत जीवन ।
         </lines>
      </verse>
   </lyrics>
</song>
