   <properties>
      <titles>
         <title>Chorus198 Aaradhana Garchu Mero</title>
      </titles>
      <authors>
         <author>Nepali Christain Bhajan</author>
      </authors>
      <songbooks>
         <songbook name="bhajan" entry="1"/>
      </songbooks>
      <verseOrder/>
   </properties>
   <lyrics>
      <verse name="v1">
         <lines>
            आराधना गर्छु मेरो जीवनभरि (२)
            <br/>
            हात उचाल्दै तपाईंको स्तुति गाउँछु (२) 
            <br/>
             तपाईंको नाउँ धन्यको भन्दछु
            <br/>
            हृदय प्राणसमेतले म गाउँदछु
            <br/>
            येशू, आराधना गर्छु (३) 
            <br/>
             मेरा ईश्वर र मेरो राजा ।
         </lines>
      </verse>
   </lyrics>
</song>
