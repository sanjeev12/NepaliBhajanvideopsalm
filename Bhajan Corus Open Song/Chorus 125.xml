<?xml version="1.0" ?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-06-12T03:38:19">
   <properties>
      <titles>
         <title>Chorus125 Mero Man Dhanya Bhan</title>
      </titles>
      <authors>
         <author>Nepali Christain Bhajan</author>
      </authors>
      <songbooks>
         <songbook name="bhajan" entry="1"/>
      </songbooks>
      <verseOrder/>
   </properties>
   <lyrics>
      <verse name="v1">
         <lines>
            मेरो मन धन्य भन ममा जो जति छन्
            <br/>
            उनको पवित्र नाउँलाई सबले धन्य भन 
            <br/>
             राजा हुन् राजाहरूका, प्रभु हुन् प्रभुहरूका
            <br/>
            राजा हुन् राजाहरूका, सधैँ सधैँका लागि ।
         </lines>
      </verse>
   </lyrics>
</song>
