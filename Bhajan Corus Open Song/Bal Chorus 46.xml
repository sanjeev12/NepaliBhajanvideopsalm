   <properties>
      <titles>
         <title>Bal Chorus46 Baibal Padha</title>
      </titles>
      <authors>
         <author>Nepali Christain Bhajan</author>
      </authors>
      <songbooks>
         <songbook name="bhajan" entry="1"/>
