<?xml version="1.0" ?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-06-12T03:38:19">
   <properties>
      <titles>
         <title>Chorus217 Dhanyako Bhanchau</title>
      </titles>
      <authors>
         <author>Nepali Christain Bhajan</author>
      </authors>
      <songbooks>
         <songbook name="bhajan" entry="1"/>
      </songbooks>
      <verseOrder/>
   </properties>
   <lyrics>
      <verse name="v1">
         <lines>
            धन्यको भन्छौँ प्रभुको नाउँलाई (२)
            <br/>
            हामी प्रेम गर्छौं येशूको नाउँलाई (२)
            <br/>
            समुद्रको बालुवा, आकाशका ताराभन्दा धेरै तपाईंको प्रेम (२) 
            <br/>
             गाउँछौँ हल्लेलूयाह सदा सदा राजा येशूलाई
            <br/>
            गाउँछौँ स्तुति महिमा सदा सदा राजा येशूलाई ।
         </lines>
      </verse>
   </lyrics>
</song>
