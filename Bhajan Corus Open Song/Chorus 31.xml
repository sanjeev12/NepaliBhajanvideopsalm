<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-06-12T03:38:19">
   <properties>
      <titles>
         <title>Chorus31 La Jau Hai Siyonsamma</title>
      </titles>
      <authors>
         <author>Nepali Christain Bhajan</author>
      </authors>
      <songbooks>
         <songbook name="bhajan" entry="1"/>
      </songbooks>
      <verseOrder>v1 c1 v2 c1 v3 c1 v4 c1 </verseOrder>
   </properties>
   <lyrics>
      <verse name="v1">
         <lines>१. ल जाऊँ है सियोनसम्म (३) येशूको नगरमा </lines>
      </verse>
      <verse name="c1">
         <lines> कोः जय हल्लेलूयाह (३) आनन्द मनाऊँ । </lines>
      </verse>
      <verse name="v2">
         <lines> २. सियोनको राजा येशू (३) राज गर्दै जाऊँ </lines>
      </verse>
      <verse name="v3">
         <lines> ३. प्रेमी छन् सियोनमा (३) प्रेम गर्दै जाऊँ </lines>
      </verse>
      <verse name="v4">
         <lines> ४. नाच्दै जाऊ सियोनमा (३) प्रेमी येशूकहाँ</lines>
      </verse>
   </lyrics>
</song>
