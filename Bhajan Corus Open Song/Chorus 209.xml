<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-06-12T03:38:19">
   <properties>
      <titles>
         <title>Chorus209 Bhayayogya Prabhu Kati</title>
      </titles>
      <authors>
         <author>Nepali Christain Bhajan</author>
      </authors>
      <songbooks>
         <songbook name="bhajan" entry="1"/>
      </songbooks>
      <verseOrder/>
   </properties>
   <lyrics>
      <verse name="v1">
         <lines>
            भययोग्य प्रभु कति महान् (४), 
            <br/>
             भर्नुहोस् मलाईं तपाईंको भयले
            <br/>
            भर्नुहोस् मलाईं तपाईंको प्रेमले,
            <br/>
            तपाईंलाई धन्यको भन्छु
            <br/>
            तपाईंको बाटो म हेर्छु (२)
         </lines>
      </verse>
   </lyrics>
</song>
